<script>
    let sitePaths = ['overview','contact']
    const reRoute = (lang) => {
        let path = window.location.pathname.split("/")
        let webPath = path[1]
        let redirectPath
        if(sitePaths.includes(webPath)) {
            redirectPath = `/${lang}`
        }else {
            redirectPath = `/${webPath}/${lang}` 
